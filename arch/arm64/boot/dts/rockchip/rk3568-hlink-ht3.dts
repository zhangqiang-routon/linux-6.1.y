&cpu_thermal {
	trips {
		cpu_cool: cpu_cool {
			temperature = <45000>;
			hysteresis = <2000>;
			type = "active";
		};

		cpu_warm: cpu_warm {
			temperature = <55000>;
			hysteresis = <2000>;
			type = "active";
		};

