		clocks = <&rk809 1>;
		clock-names = "lpo";
		device-wakeup-gpios = <&gpio3 RK_PA1 GPIO_ACTIVE_HIGH>;
		host-wakeup-gpios = <&gpio3 RK_PA2 GPIO_ACTIVE_HIGH>;
		shutdown-gpios = <&gpio3 RK_PA0 GPIO_ACTIVE_HIGH>;
